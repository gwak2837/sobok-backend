{
  "name": "sobok-backend",
  "version": "0.1.0",
  "description": "sobok-backend",
  "homepage": "https://github.com/teamsindy20/sobok-backend#readme",
  "bugs": {
    "url": "https://github.com/teamsindy20/sobok-backend/issues",
    "email": "teamsindy20@gmail.com"
  },
  "license": "GPL-3.0-only",
  "author": "sindy",
  "main": "dist/index.js",
  "repository": "https://github.com/teamsindy20/sobok-backend.git",
  "scripts": {
    "old-dev": "NODE_ENV=development ts-node -r dotenv/config src/graphql/schema.ts dotenv_config_path=.env.development --trace-warnings",
    "dev": "NODE_ENV=development concurrently \"yarn webpack\" \"nodemon -r dotenv/config dist/index.js dotenv_config_path=.env.development --trace-warnings\"",
    "build": "yarn generate && yarn generate-db && NODE_ENV=production yarn webpack",
    "start": "NODE_ENV=production node -r dotenv/config dist/index.js",
    "test": "NODE_ENV=test ts-node -r dotenv/config test/index.ts dotenv_config_path=.env.test --trace-warnings --watch test",
    "format": "prettier . --write --ignore-path .gitignore",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "type": "tsc --pretty --noEmit",
    "pre-push": "yarn format && yarn lint && yarn type",
    "generate": "rm -rf src/graphql/generated && graphql-codegen --config codegen.yml",
    "generate-db": "schemats postgres postgres://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}/${POSTGRES_DB} -o src/database/types.ts && prettier src/database/types.ts --write",
    "postinstall": "[ -d '.husky' ] && husky install || true"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.4.0",
    "@graphql-tools/merge": "^8.1.2",
    "@graphql-tools/schema": "^8.2.0",
    "apollo-server-core": "^3.3.0",
    "apollo-server-express": "^3.3.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.10.1",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.7.1",
    "pg-format": "^1.0.4",
    "redis": "^3.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/schema-ast": "^2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-resolvers": "2.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/graphql-fields": "^1.3.4",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.9.1",
    "@types/passport": "^1.0.7",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-google-oauth20": "^2.0.9",
    "@types/pg": "^8.6.1",
    "@types/pg-format": "^1.0.2",
    "@types/redis": "^2.8.32",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vramework/schemats": "^0.0.7",
    "concurrently": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.1",
    "jest": "^27.1.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.9.0"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "private": true
}
